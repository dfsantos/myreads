{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": ["react", "import", "prettier"],
  "overrides": [
    {
      "files": ["**/__tests__/*.js", "**/*.test.js", "**/*.spec.js"],
      "rules": {
        "react/jsx-filename-extension": [0]
      }
    }
  ]
}
